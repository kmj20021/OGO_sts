<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TutorMapper">

    <insert id="insertTutor" parameterType="TutorDTO">
        insert into TUTOR(timg,
                          tname,
                          tcertificate,
                          tcategory_id,
                          tintroduce,
                          userid)
        values (#{timg},
                #{tname},
                #{tcertificate},
                #{tcategory_id},
                #{tintroduce},
                #{userid})
    </insert>


</mapper>
